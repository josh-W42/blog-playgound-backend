{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "lib": ["ES2018", "ES2020"],
    "target": "es2020",
    "module": "ESNext"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
